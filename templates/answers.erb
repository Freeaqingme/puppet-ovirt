# action=setup
[environment:default]
OVESETUP_CORE/engineStop=none:None
OVESETUP_DIALOG/confirmSettings=bool:True
OVESETUP_DB/database=str:engine
OVESETUP_DB/fixDbViolations=none:None
OVESETUP_DB/secured=bool:False
OVESETUP_DB/host=str:<%= @dbHost %>
OVESETUP_DB/user=str:engine
OVESETUP_DB/securedHostValidation=bool:False
OVESETUP_DB/password=str:<%= @dbPassword %>
OVESETUP_DB/port=int:<%= @dbPort %>
OVESETUP_SYSTEM/nfsConfigEnabled=bool:<%= @nfsConfigEnabled ? "True" : "False" %>
OVESETUP_PKI/organization=str:<%= @organization %>
OVESETUP_CONFIG/isoDomainName=str:<%= @isoDomainName %>
OVESETUP_CONFIG/isoDomainMountPoint=str:<%= @isoDomainMountPoint %>
OVESETUP_CONFIG/adminPassword=str:<%= @adminPassword %>
OVESETUP_CONFIG/applicationMode=str:<%= @applicationMode %>
OVESETUP_CONFIG/firewallManager=str:<%= @firewallManager %>
OVESETUP_CONFIG/websocketProxyConfig=none:None
OVESETUP_CONFIG/fqdn=str:<%= @fqdn %>
OVESETUP_CONFIG/storageType=str:<%= @storageType %>
OVESETUP_PROVISIONING/postgresProvisioningEnabled=bool:True
OVESETUP_APACHE/configureRootRedirection=bool:True
OVESETUP_APACHE/configureSsl=bool:True
OSETUP_RPMDISTRO/requireRollback=none:None
OSETUP_RPMDISTRO/enableUpgrade=none:None
OVESETUP_AIO/configure=none:None
OVESETUP_AIO/storageDomainDir=none:None
